		{
			"name": "Set Environment Variables",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "ee4b9fd6-04f5-452f-bea1-7ef4df2ec707",
						"exec": [
							"/** ",
							" * Please make sure to comply with our Terms and Conditions (see https://github.com/BoschSmartHome/bosch-shc-api-docs)",
							" * by using the Bosch Smart Home Controller API",
							" * ",
							" * ",
							" * ",
							" * Hit 'Send' to set the following environment variables. ",
							" * ",
							" **/",
							" ",
							" ",
							" /**",
							"  * ",
							"  * Edit IP of your Smart Home Controller.",
							"  * ",
							"  **/",
							"postman.setEnvironmentVariable(\"host\", \"192.168.0.10\");",
							" ",
							"/**",
							" * ",
							" * URLs of Smart Home Controller. Do not edit.",
							" * ",
							" **/",
							"postman.setEnvironmentVariable(\"shc_secure\", \"https://{{host}}:8443/smarthome/clients\");",
							"postman.setEnvironmentVariable(\"shc_poll\", \"https://{{host}}:8444/remote/json-rpc\");",
							"postman.setEnvironmentVariable(\"shc_api\", \"https://{{host}}:8444/smarthome\");",
							"",
							"/** ",
							" * Header",
							" * ",
							" * @param system_password_base64 - Set your master password of your Smart Home Controller base64 encoded here",
							" **/",
							"postman.setEnvironmentVariable(\"system_password_base64\", \"\");",
							"",
							"/** ",
							" * Body",
							" * ",
							" * @param client_id - Name of your open source project with prefix 'oss_'. Optional a unique id can be added as a suffix. E.g. oss_my-project_788c22fb-b090-4faf-8faf-15f67b9128c2 ",
							" * @param client_name - Name of your open source project with prefix 'OSS'. E.g. OSS My Project",
							" **/",
							"postman.setEnvironmentVariable(\"client_id\", \"oss_postman\");",
							"postman.setEnvironmentVariable(\"client_name\", \"OSS Postman\");",
							"",
							"",
							"/** ",
							" * Scenario Id",
							" * ",
							" * @param scenario_id - The id of the scenario",
							" **/",
							"postman.setEnvironmentVariable(\"scenario_id\", \"\");",
							"",
							"/** ",
							" * Room Id",
							" * ",
							" * @param room_id - The id of the room",
							" **/",
							"postman.setEnvironmentVariable(\"room_id\", \"\");",
							"",
							"/** ",
							" * Device Id",
							" * ",
							" * @param device_id - The id of the device",
							" **/",
							"postman.setEnvironmentVariable(\"device_id\", \"\");",
							"",
							"/** ",
							" * Service Id",
							" * ",
							" * @param service_id - The id of the service",
							" **/",
							"postman.setEnvironmentVariable(\"service_id\", \"\");",
							"",
							"",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
